  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "chalk": "^2.4.2",
    "contentful-import": "^7.6.1",
    "gatsby": "^2.18.3",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-canonical-urls": "^2.1.15",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-feed": "^2.3.21",
    "gatsby-plugin-google-analytics": "^2.1.28",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-netlify": "^2.1.26",
    "gatsby-plugin-nprogress": "^2.1.14",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sitemap": "^2.2.21",
    "gatsby-plugin-styled-components": "^3.1.13",
    "gatsby-remark-autolink-headers": "^2.1.18",
    "gatsby-remark-images-contentful": "^2.1.24",
    "gatsby-remark-prismjs": "^3.3.24",
    "gatsby-source-contentful": "^2.1.63",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-remark": "^2.6.37",
    "global": "^4.4.0",
    "inquirer": "^6.5.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "npm run build && gatsby serve",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "setup": "node ./bin/setup.js",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier --list-different {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "prettier": "^1.19.1",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0"
  }
}
